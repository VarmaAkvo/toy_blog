<tr>
  <td>
    <%= report.reporter.name %>
  </td>
  <td>
    <%= report.reported.name %>
  </td>
  <td>
    <%= report.reportable_type %>
  </td>
  <td>
    <%= report.created_at.strftime('%Y年%m月%d日 %H:%M') %>
  </td>
  <td>
    <%= link_to '详细', admin_report_path(report), class: 'btn btn-info btn-sm' %>
  </td>
</tr>
